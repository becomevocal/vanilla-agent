{
  "name": "vanilla-agent-proxy",
  "version": "1.17.0",
  "description": "Proxy server for vanilla-agent widget - handles flow configuration and forwards requests to Travrse or other AI backends.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.4.9"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "license": "MIT",
  "keywords": [
    "proxy",
    "server",
    "travrse",
    "chat",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becomevocal/chaty.git"
  }
}
